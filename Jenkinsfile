/* Requires the Docker Pipeline plugin */
pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'building step'
            }
        }
        
        stage('test') {
            steps {
                echo 'test step'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy step'
            }
        }
        
    }
}
